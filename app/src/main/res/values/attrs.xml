<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="seekbar">
    <attr name="mode">
        <enum name="horizontal" value="1"/>
        <enum name="vertical" value="2"/>
    </attr>
    <attr name="base_margin_left" format="dimension|reference"/>
    <attr name="base_margin_right" format="dimension|reference"/>
    <attr name="base_margin_top" format="dimension|reference"/>
    <attr name="base_margin_bottom" format="dimension|reference"/>
    <attr name="base_background" format="color|reference"/>
    <attr name="item_width" format="dimension|reference"/>
    <attr name="item_height" format="dimension|reference"/>
    <attr name="anchor_width" format="dimension|reference"/>
    <attr name="anchor_height" format="dimension|reference"/>
    <attr name="draw_on_off" format="boolean"/>
    <attr name="fix_point" format="boolean"/>
</declare-styleable>

</resources>